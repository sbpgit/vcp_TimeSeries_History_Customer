<core:FragmentDefinition id="_IDGenFragmentDefinition1" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
    <Dialog id="openDialogTHC" title="Create Variant" afterClose="onAfterClose">
        <f:SimpleForm id="createVariantTHC" visible="true" editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" emptySpanL="6" columnsL="1">
            <f:content>
                <VBox id="_IDGenVBox123THC">
                    <VBox id="_IDGenVBox2THC">
                        <Label id="_IDGenLabel1THC" text="View:" />
                        <Input value="" id="idInputTHC" liveChange="checkName" valueStateText="The view name already exists.Please enter a different name."/>
                    </VBox>
                    <CheckBox id="_IDGenCheckBox1THC" text="Set as Default" />
                    <!-- <CheckBox id="_IDGenCheckBox2" text="Public" /> -->
                    <CheckBox id="_IDGenCheckBox3THC" text="Apply Automatically" visible="false" />
                </VBox>
            </f:content>
        </f:SimpleForm>
        <buttons>
            <Button id="idSaveBtnTHC" text="Save" press="onCreate" type="Emphasized"/>
            <Button id="idCanBtnTHC"  text="Close" press="onSaveClose" />
        </buttons>
    </Dialog>
</core:FragmentDefinition>
