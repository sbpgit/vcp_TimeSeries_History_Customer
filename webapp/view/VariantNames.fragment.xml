<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog id="_IDGenDialog1THC" title="Manage Views" contentWidth="850px" titleAlignment="Center">
        <Toolbar id="IDGenToolbar1THC">
            <SearchField id="_IDGenSearchField1THCTHC" width="100%" liveChange="handleSearch" placeholder="View Name/User" />
        </Toolbar>
        <Table id="varNameListTHC" items="{path: '/items1'}" >
            <columns>
                <Column id="_IDGenColumn1THC">
                    <Text id="_IDGenText1THC" text="View" />
                </Column>
                <!-- <Column id="_IDGenColumn2">
                    <Text id="_IDGenText2" text="Public/Private" />
                </Column> -->
                <Column id="_IDGenColumn3THC">
                    <Text id="_IDGenText3THC" text="Default" />
                </Column>
                <!-- <Column id="_IDGenColumn4">
                    <Text id="_IDGenText4" text="Default" />
                </Column> -->
                
                <Column id="_IDGenColumn6THC" width="300px">
                    <Text id="_IDGenText6THC" text="Created By" />
                </Column>
                <Column id="_IDGenColumn5THC" visible="false">
                    <Text id="_IDGenText5" text="Variant ID" />
                </Column>
                <Column id="_IDGenColumn7THC" visible="true" hAlign="End">
                    <Text id="_IDGenText7THC" text="" />
                </Column>
            </columns>
            <items>
                <ColumnListItem >
                    <cells>
                        <!-- <ObjectIdentifier title="{VARIANTNAME}" text="{APPLICATION_NAME}"  titleActive="true" titlePress="onTitlePress"/> -->
                        <!-- <Text id="_IDGenText9" text="{VARIANTID}" /> -->
                        <Text  text="{VARIANTNAME}" />
                        <!-- <Text id="_IDGenText11" text="{SCOPE}" /> -->
                        <RadioButton  selected="{= ${DEFAULT} === 'Y'}" />
                        <Text  text="{USER}" />
                        <Text  text="{VARIANTID}" />
                        <Button  icon="sap-icon://decline" visible="{=${VARIANTNAME}!=='Standard'}" press="onViewDelete" />
                        <!-- <Text id="_IDGenText14" text="{FIELD_CENTER}" /> -->
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <buttons>
            <Button id="_IDGenButtTHC" type='Emphasized' text="Save" press="onManage" />
            <Button id="IDGenButton2THC"  text="Close" press="handleManageClose" />
        </buttons>
    </Dialog>
</core:FragmentDefinition>
